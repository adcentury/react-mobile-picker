    "babel-loader": "^8.2.5",
    "eslint": "^8.14.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-react": "^7.29.4",
    "eslint-webpack-plugin": "^3.1.1",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.8.1",
