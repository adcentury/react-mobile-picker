{
  "name": "react-mobile-picker",
  "version": "0.1.13",
  "description": "An iOS like select box component for React",
  "main": "lib/react-mobile-picker.js",
  "repository": {
    "type": "git",
    "url": "adcentury/react-mobile-picker"
